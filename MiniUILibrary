local ts: TweenService = game:GetService("TweenService")
local UIS: UserInputService = game:GetService('UserInputService')
local core_gui: CoreGui | StarterGui = game['CoreGui'] or game['Players']['LocalPlayer']['PlayerGui']
-- Gui without tabs!

local main_tab: ScrollingFrame

local function load_dropdown(scroll: ScrollingFrame, children, signal)
	if not children then warn('[SpineLibrary]: Hey! Children are nil! Dropdown can not be filled by nil.') return end
		
	for _, child: Model in children do 
		local TextButton = Instance.new("TextButton")
		local UICorner_9 = Instance.new("UICorner")
		
		TextButton.Parent = scroll
		TextButton.BackgroundColor3 = Color3.fromRGB(94, 94, 94)
		TextButton.BackgroundTransparency = 0.200
		TextButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
		TextButton.BorderSizePixel = 0
		TextButton.Position = UDim2.new(0.0678851157, 0, 0, 0)
		TextButton.Size = UDim2.new(0, 348, 0, 21)
		TextButton.AutoButtonColor = false
		TextButton.Font = Enum.Font.Jura
		TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
		TextButton.TextScaled = true
		TextButton.TextSize = 14.000
		TextButton.TextWrapped = true
		
		if child:IsA("Model") or child:IsA("MeshPart") or child:IsA("UnionOperation") or child:IsA("Part") or child:IsA("BasePart") then
			TextButton.Text = child.Name
		elseif child:IsA("BoolValue") or child:IsA("StringValue") or child:IsA("IntValue") or child:IsA("ObjectValue") then
			TextButton.Text = child.Value
		end
		
		UICorner_9.CornerRadius = UDim.new(0, 4)
		UICorner_9.Parent = TextButton
		
		scroll.CanvasSize = #children > 5 and scroll.CanvasSize + UDim2.new(0, 0, 0.4, 0) or UDim2.new(0, 0, 2, 0)
		
		TextButton.MouseButton1Click:Connect(function()
			task.spawn(function()
				if not signal then warn("[SpineLibrary]: Signal was not connected (at dropdown!)") return end
				signal(TextButton.Text)
			end)
			
			for i, drop_variant: TextButton in scroll:GetChildren() do 
				if drop_variant:IsA("TextButton") then
					ts:Create(drop_variant, TweenInfo.new(0.7), {BackgroundColor3 = Color3.new(0.368627, 0.368627, 0.368627)}):Play()
				end
			end
			ts:Create(TextButton, TweenInfo.new(0.75),{BackgroundColor3 = Color3.new(0.211765, 0.639216, 0.164706)}):Play()
		end)
	end
end

