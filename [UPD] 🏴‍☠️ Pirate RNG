local function DSMFRO_fake_script() -- Trigger.OnActivate 
	local script = Instance.new('LocalScript', Trigger)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,18,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local Players = game:GetService("Players")
				local chestsFolder = workspace:WaitForChild("Main Area"):WaitForChild("Chests")
	
				local function teleportPlayerToRandomChest(player)
					if not chestsFolder then
						warn("–ü–∞–ø–∫–∞ 'Chests' –Ω–µ –Ω–∞–π–¥–µ–Ω–∞.")
						return
					end
	
					local chests = chestsFolder:GetChildren()
					if #chests == 0 then
						warn("–í –ø–∞–ø–∫–µ 'Chests' –Ω–µ—Ç –æ–±—ä–µ–∫—Ç–æ–≤.")
						return
					end
	
					local randomIndex = math.random(1, #chests)
					local randomChest = chests[randomIndex]
					local winPart = randomChest
					if not winPart:IsA("BasePart") then
						winPart = randomChest:FindFirstChildWhichIsA("BasePart")
						if not winPart then
							warn("–ù–µ —É–¥–∞–ª–æ—Å—å –Ω–∞–π—Ç–∏ BasePart –≤ —Å—É–Ω–¥—É–∫–µ " .. randomChest.Name)
							return
						end
					end
					local winCFrame = winPart.CFrame
	
					local char = player.Character
					if char and char:FindFirstChild("HumanoidRootPart") then
						char:FindFirstChild("HumanoidRootPart").CFrame = winCFrame * CFrame.new(0, 2, 0)
					end
				end
	
				local function onCharacterAdded(character)
					local player = Players:GetPlayerFromCharacter(character)
					if not player then return end
					teleportPlayerToRandomChest(player)
				end
	
	
				-- –ö–æ–≥–¥–∞ –∏–≥—Ä–æ–∫ –ø—Ä–∏—Å–æ–µ–¥–∏–Ω–∏–ª—Å—è
				Players.PlayerAdded:Connect(function(player)
					player.CharacterAdded:Connect(onCharacterAdded)
				end)
				-- –ò –µ—Å–ª–∏ –æ–Ω —É–∂–µ –±—ã–ª –≤ –∏–≥—Ä–µ.
				for _, player in ipairs(Players:GetPlayers()) do
					local character = player.Character
					if character then
						onCharacterAdded(character)
					end
				end
			end) 
			wait()
		end
	end)
end
coroutine.wrap(DSMFRO_fake_script)()
local function QTXE_fake_script() -- Trigger_2.LocalScript 
	local script = Instance.new('LocalScript', Trigger_2)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.3}):Play()
	end)
	
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.5}):Play()
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		local Players = game:GetService("Players")
		local targetCFrame = CFrame.new(-7.62300014, 131.067688, -31.7250004)
	
		local function teleportPlayer(player)
			local character = player.Character
			if character and character:FindFirstChild("HumanoidRootPart") then
				local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
				humanoidRootPart.CFrame = targetCFrame  * CFrame.new(0, 2, 0) -- –î–æ–±–∞–≤–ª–µ–Ω–æ —Å–º–µ—â–µ–Ω–∏–µ –Ω–∞ 2 —é–Ω–∏—Ç–∞
			end
		end
	
		local function onCharacterAdded(character)
			local player = Players:GetPlayerFromCharacter(character)
			if player then
				teleportPlayer(player)
			end
		end
	
		-- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º –ø—Ä–∏ –¥–æ–±–∞–≤–ª–µ–Ω–∏–∏ –∏–≥—Ä–æ–∫–∞.
		Players.PlayerAdded:Connect(function(player)
			player.CharacterAdded:Connect(onCharacterAdded)
		end)
	
		-- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º —É–∂–µ —Å—É—â–µ—Å—Ç–≤—É—é—â–∏—Ö –∏–≥—Ä–æ–∫–æ–≤.
		for _, player in ipairs(Players:GetPlayers()) do
			local character = player.Character
			if character then
				onCharacterAdded(character)
			end
		end
	end)
	if not sucess then
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.709804, 0.113725, 0.0705882)}):Play()
		wait(2)
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.219608, 0.219608, 0.219608)}):Play()		
	else
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.12549, 0.615686, 0.141176)}):Play()
		wait(2)
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.219608, 0.219608, 0.219608)}):Play()
	end
end
coroutine.wrap(QTXE_fake_script)()
local function WXUV_fake_script() -- TextLabel_3.Script 
	local script = Instance.new('LocalScript', TextLabel_3)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1)
	script.Parent.Font = Enum.Font.Michroma
	script.Parent.MouseEnter:Connect(function()
		pcall(function()
			ts:Create(script.Parent,info,{TextColor3 = Color3.new(math.random(0.1,1),math.random(0.1,1),math.random(0.1,1))}):Play()
		end)
	end)
	
	script.Parent.MouseLeave:Connect(function()
		pcall(function()
			ts:Create(script.Parent,info,{TextColor3 = Color3.new(1,1,1)}):Play()
		end)
	end)
end
coroutine.wrap(WXUV_fake_script)()
local function WRMJNY_fake_script() -- Toggle.OnActivate 
	local script = Instance.new('LocalScript', Toggle)

	local toggle = script.Parent
	local ts= game:GetService("TweenService")
	local info = TweenInfo.new(1.25,Enum.EasingStyle.Cubic)
	
	local status = false
	local isalreadydoing = false	
	
	script.Parent.MouseButton1Click:Connect(function()
		if isalreadydoing then return end
		local sucess, err = pcall(function()
			local closingtab = script.Parent.Parent.Parent
			
			if not status then
			isalreadydoing = true
			status = true
			ts:Create(script.Parent,TweenInfo.new(1,Enum.EasingStyle.Cubic),{Rotation =180}):Play()
			local tween = ts:Create(closingtab,info,{Size = UDim2.new(0,210,0,35)})
			local x = ts:Create(closingtab.Container,TweenInfo.new(0.5),{BackgroundTransparency = 1})
			closingtab.Container.ContainerList.Visible = false
			tween:Play()
			x:Play()
			x.Completed:Wait()
			tween.Completed:Wait()
			isalreadydoing = false
			else
				status = false
				isalreadydoing = true
				local tween = ts:Create(closingtab,info,{Size = UDim2.new(0,210,0,276)})
				ts:Create(script.Parent,TweenInfo.new(1,Enum.EasingStyle.Cubic),{Rotation =0}):Play()
				tween:Play()
				tween.Completed:Wait()
				ts:Create(closingtab.Container,TweenInfo.new(0.5),{BackgroundTransparency = 0.5}):Play()
				isalreadydoing = false
				closingtab.Container.ContainerList.Visible = true
			end
		end)
		
		if not sucess then print(err) end
	end)
end
coroutine.wrap(WRMJNY_fake_script)()
local function YGHH_fake_script() -- ImageButton.LocalScript 
	local script = Instance.new('LocalScript', ImageButton)

	script.Parent.MouseButton1Click:Connect(function()
		pcall(function()
			setclipboard("https://discord.gg/Pk7VQqZvZr")
		end)
	end)
end
coroutine.wrap(YGHH_fake_script)()
local function KOUJJO_fake_script() -- ImageLabel.LocalScript 
	local script = Instance.new('LocalScript', ImageLabel)

	script.Parent.Active= true
	script.Parent.Draggable =true
	
	pcall(function()
		for _, v in pairs(script.Parent:GetDescendants()) do
			if v:IsA("ScrollingFrame") then
				v.ScrollBarImageTransparency =1 
			end
		end
	end)
	
	
end
coroutine.wrap(KOUJJO_fake_script)()
local function OAOSV_fake_script() -- RNGTEST.Tween 
	local script = Instance.new('LocalScript', RNGTEST)

	local list = {
	
	}
	
	local sucess, err = pcall(function()
		--—ç–ª–µ–º–µ–Ω—Ç—ã —É–±–∏—Ä–∞—é—Ç —É —Å–µ–±—è transparency
		for _, e in pairs(script.Parent:GetDescendants()) do
			--–ø—Ä–æ–≤–µ—Ä–∫–∞
			if e:IsA("TextBox") or e:IsA("TextLabel") or e:IsA("TextButton") then
				table.insert(list,{e,e.BackgroundTransparency,e.TextTransparency})
				e.BackgroundTransparency = 1
				e.TextTransparency = 1
			elseif e:IsA("ImageLabel") or e:IsA("ImageButton") then
				table.insert(list,{e,e.BackgroundTransparency,e.ImageTransparency})
				e.BackgroundTransparency = 1
				e.ImageTransparency = 1
			elseif e:IsA("Frame") or e:IsA("ScrollingFrame") then
				table.insert(list,{e,e.BackgroundTransparency})
				e.BackgroundTransparency = 1
			else
	
			end
		end
	end)
	
	if not sucess then print(err) end
	
	local a,b = pcall(function()
		wait(3.5)
		--–≤—ã—Å—Ç–∞–≤–ª–µ–Ω–∏–µ –∞—Ä–≥—É–º–µ–Ω—Ç–æ–≤
		local ts = game:GetService("TweenService")
		local info = TweenInfo.new(3.5)
	
		for _, t in pairs(list) do
			pcall(function()
				if t[1]:IsA("TextBox") or t[1]:IsA("TextLabel") or t[1]:IsA("TextButton") then
					ts:Create(t[1],info,{BackgroundTransparency = t[2]}):Play()
					ts:Create(t[1],info,{TextTransparency = t[3]}):Play()
					if t[3] == nil then
						ts:Create(t[1],info,{TextTransparency = 0})
					end
				elseif t[1]:IsA("ImageLabel") or t[1]:IsA("ImageButton") then
					ts:Create(t[1],info,{BackgroundTransparency = t[2]}):Play()
					ts:Create(t[1],info,{ImageTransparency = t[3]}):Play()
				elseif t[1]:IsA("ScrollingFrame") or t[1]:IsA("Frame") then
					ts:Create(t[1],info,{BackgroundTransparency = t[2]}):Play()       
				end
			end)
		end
	end)
	
	if not a then print(b) end
end
coroutine.wrap(OAOSV_fake_script)()
