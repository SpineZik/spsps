-- Scripts:

local function ZLKE_fake_script() -- Trigger.LocalScript 
	local script = Instance.new('LocalScript', Trigger)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.3}):Play()
	end)
	
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.5}):Play()
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		local sucess, err =pcall(function()
			local Event = game:GetService("ReplicatedStorage").Events.DamageIncreaseOnClickEvent
			Event:FireServer()
		end)
		
		if not sucess then
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.709804, 0.113725, 0.0705882)}):Play()
			wait(2)
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.219608, 0.219608, 0.219608)}):Play()		
		else
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.12549, 0.615686, 0.141176)}):Play()
			wait(2)
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.219608, 0.219608, 0.219608)}):Play()
		end
	end)
end
coroutine.wrap(ZLKE_fake_script)()
local function AUGIW_fake_script() -- Trigger_2.LocalScript 
	local script = Instance.new('LocalScript', Trigger_2)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.3}):Play()
	end)
	
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.5}):Play()
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		local sucess, err =pcall(function()
			local choosedboss = _G.EggToHatch
			local stage = _G.Stage
			
			local A_1 = "Fight"
			local A_2 = stage
			local A_3 = choosedboss 
			local Event = game:GetService("ReplicatedStorage").Events.AutoFight
			Event:FireServer(A_1, A_2, A_3)
			local A_1 = true
			local Event = game:GetService("ReplicatedStorage").Events.CaughtFish
			Event:FireServer(A_1)
			local A_1 = false
			local Event = game:GetService("ReplicatedStorage").Events.StartFishing
			Event:FireServer(A_1)
		end)
		
		if not sucess then
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.709804, 0.113725, 0.0705882)}):Play()
			wait(2)
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.219608, 0.219608, 0.219608)}):Play()		
		else
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.12549, 0.615686, 0.141176)}):Play()
			wait(2)
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.219608, 0.219608, 0.219608)}):Play()
		end
	end)
end
coroutine.wrap(AUGIW_fake_script)()
local function TCSA_fake_script() -- Trigger_3.OnActivate 
	local script = Instance.new('LocalScript', Trigger_3)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,18,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local A_1 = "Fight"
				local A_2 = _G.Stage
				local A_3 = _G.EggToHatch 
				local Event = game:GetService("ReplicatedStorage").Events.AutoFight
				Event:FireServer(A_1, A_2, A_3)
				local A_1 = true
				local Event = game:GetService("ReplicatedStorage").Events.CaughtFish
				Event:FireServer(A_1)
				local A_1 = false
				local Event = game:GetService("ReplicatedStorage").Events.StartFishing
				Event:FireServer(A_1)
			end)
			game:GetService("RunService").Heartbeat:Wait()
		end
	end)
end
coroutine.wrap(TCSA_fake_script)()
local function FLRWKRM_fake_script() -- Trigger_4.OnActivate 
	local script = Instance.new('LocalScript', Trigger_4)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,18,0,0)}):Play()
			status = true
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			local Event = game:GetService("ReplicatedStorage").Events.DamageIncreaseOnClickEvent
			Event:FireServer()
			game:GetService("RunService").Heartbeat:Wait()
		end
	end)
end
coroutine.wrap(FLRWKRM_fake_script)()
local function XXYYKH_fake_script() -- TextBox.OnActivate 
	local script = Instance.new('LocalScript', TextBox)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.Focused:Connect(function()
		pcall(function()
			ts:Create(script.Parent,info,{BackgroundColor3 = Color3.new(0.439216, 0.439216, 0.439216)}):Play()
		end)
	end)
	
	script.Parent.FocusLost:Connect(function()
		local sucess, err = pcall(function()
			ts:Create(script.Parent,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			local textbox = script.Parent
			local text = textbox.Text
			
			local tonum = tonumber(text)
			
			if tonum ~= nil then
				_G.Stage = tonum
			else
				ts:Create(textbox,x,{BackgroundColor3 = Color3.new(0.698039, 0.0705882, 0.164706)}):Play()
				wait(1)
				ts:Create(textbox,x,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			end
		end)
		
		if not sucess then print(err) end
	end)
end
coroutine.wrap(XXYYKH_fake_script)()
local function YCRBC_fake_script() -- TextBox_2.OnActivate 
	local script = Instance.new('LocalScript', TextBox_2)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.Focused:Connect(function()
		pcall(function()
			ts:Create(script.Parent,info,{BackgroundColor3 = Color3.new(0.439216, 0.439216, 0.439216)}):Play()
		end)
	end)
	
	script.Parent.FocusLost:Connect(function()
		local sucess, err = pcall(function()
			ts:Create(script.Parent,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			local textbox = script.Parent
			local text = textbox.Text
			
			local tonum = tonumber(text)
			
			if tonum ~= nil then
				_G.EggToHatch = tonum
			else
				ts:Create(textbox,x,{BackgroundColor3 = Color3.new(0.698039, 0.0705882, 0.164706)}):Play()
				wait(1)
				ts:Create(textbox,x,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			end
		end)
		
		if not sucess then print(err) end
	end)
end
